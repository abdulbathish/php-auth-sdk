{
    "name": "mosip/php-auth-sdk",
    "description": "PHP SDK for MOSIP Authentication",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "MosipAuth\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
